])                          #visited city , path cost\n","      #print(next_vist, dis[visited] + next_vist_dis)        #next visited city , path cost\n","      #print(dis[next_vist])\n","\n","      if dis[next_vist] > (dis[visited] + next_vist_dis) :\n","        dis[next_vist] = dis[visited] + next_vist_dis\n","        h_dis[next_vist] = dis[next_vist] + heu[next_vist]   #f(n) updated\n","        #print(next_vist, dis[next_vist], h_dis[next_vist])\n","        p_node[next_vist] = des[1]\n","        #print(p_node)\n","        qu.put((h_dis[next_vist], next_vist))\n","\n","      if des == goal:\n","        break\n","\n","  if dis[goal] == math.inf:\n","     print(\"No path found\")\n","\n","  else:\n","      reached = goal\n","      path = \"\"\n","      while reached != start :\n","          if reached == goal :\n","            path = reached + path\n","            #print(path)\n","          else:\n","            path = reached + \" -> \" + path\n","            #print(path)\n","\n","          reached = p_node[reached]\n","          #print(reached)\n","\n","      path = start + \" -> \" + path\n","\n","      if path == \"\":\n","          print(\"No path found\")\n","\n","      else:\n","         print(\"Path: \"+ path)\n","         print(\"Total Distance : \" + str(dis[goal])+\" km\")\n","\n","#A_Star_Search(grp,'Arad','Bucharest')\n","\n","A_Star_Search(grp,'Arad','Neamt')\n","#A_Star_Search(grp,'Arad','Dobreta')\n","#A_Star_Search(grp,'Arad','Eforie')\n"],"metadata":{"id":"RxK7yxbWstTx","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1690702574529,"user_tz":-360,"elapsed":14,"user":{"displayName":"Abeda Zahid Chandrica","userId":"08111882696983234900"}},"outputId":"2c5e36b9-c3c5-41f5-c7b3-40d5570d4ae0"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Path: Arad -> Sibiu -> RimnicuVilcea -> Pitesti -> Bucharest -> Urziceni -> Vaslui -> lasi -> Neamt\n","Total Distance : 824 km\n"]}]},{"cell_